
ms = []
ais = []

for n in range(4, 25):
    m = [0 for _ in range(n)]
    a = [0 for _ in range(n)]
    
    for i in range(n):
        
        p = random_prime(2^8-1,False,2^3)
        while p in m:
            p = random_prime(2^8-1,False,2^3)
        m[i] = p
    for i in range(n):
        a[i] = prod([m[j] for j in range(n) if j != i])%m[i]
        
    ms.append(m)
    ais.append(a)

